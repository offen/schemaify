    "ajv-pack": "^0.3.1",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "@studio/changes": "^2.0.1",
    "browserify": "^16.5.1",
    "standard": "^14.3.4",
    "tape": "^5.0.1",
    "vm": "^0.1.0"
  },
  "standard": {
